@model CompassTutor.Models.SubjectType
@{
    ViewData["Title"] = "Create";
     ViewBag.Pagename = "Subject Topic";

}

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.css">

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-circle-progress/1.1.3/circle-progress.min.js"></script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<div class="page-wrapper" style="min-height: 638px;padding-top:0px;">
    <div class="content container-fluid">
        <div class="card-box" style="padding-bottom:0px !important">
            <div class="panel">
                <div class="panel-heading" style="background-color:darkgrey;"><h3 class="card-title" style="margin-bottom:0px !important;font-size:20px;color:white">Add Specify Subject Topic</h3></div>
                <div class="panel-body">
                    <div class="row" style="margin-top:10px;">
                        <div class="col-md-6">
                            <div class="form-group select-focus form-focus subjectdd1" style="margin-bottom:0px">
                                <label class="control-label">Subject</label>
                                <select class="select form-control floating" id="subjectdd" value="">
                                    <option value="">Select</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="margin-top:15px">
                        <div class="col-md-6">
                            <div class="form-group form-focus SubjectTopic1" style="margin-bottom:0px;">
                                <label class="control-label" for="SubjectTopic">Specify Subject Topic</label>
                                <input type="text" id="SubjectTopic" class="form-control floating" value="" required />
                                @*<span class="error_duplicate" style="display:none;color:red">Subject Topic Already Exist</span>*@
                                @*<span class="small">&nbsp;<span>( Hint: If Subject Contains more than one topic, enter topic as ' , ' seperated )&nbsp;</span></span>*@
                            </div>
                            <span class="error_duplicate" style="display:none;color:red">Specify Subject Topic Already Exist</span>
                        </div>
                    </div>
                    <div>
                        <button class="btn btn-primary btn-lg" style="width:100px;" onclick="Save()" type="button" id="btnSaveSubjectTopic">Save</button>
                        <a href="@Url.Action("Index","SubjectTopic")" style="width:100px;" class="btn btn-danger btn-lg">Cancel</a>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
<link href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet">
<script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
<script type="text/javascript" src="~/js/jquery.dataTables.min.js"></script>
<script src="~/js/SubjectTopic/SubjectTopicMaster.js"></script>




